# =============================
# Laravel / PHP Standard .gitignore
# =============================

# Dependencies
/vendor/
/node_modules/

# Environment files
/.env
/.env.*
!.env.example

# Sensitive storage keys
/storage/*.key

# Logs & runtime files
/storage/logs/*
!/storage/logs/.gitignore
/storage/debugbar/*

# Framework generated files (cache, sessions, views)
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
!/storage/framework/.gitignore

# Public storage (symlink)
/public/storage

# Build artifacts
/public/hot
/public/build

# Bootstrap cache (compiled) - ignore compiled files but keep folder
/bootstrap/cache/*.php

# Composer PHAR
/composer.phar

# PHPUnit cache
/.phpunit.result.cache

# Vite / tooling
/node_modules/.vite

# OS generated files
.DS_Store
Thumbs.db
desktop.ini

# Logs from package managers
npm-debug.log*
yarn-error.log*
pnpm-debug.log*

# IDE / Editor
/.idea/
/.vscode/
.phpstorm.meta.php

# Optional files you might not want to commit
.env.testing
.env.backup

# Misc
.sass-cache/
.cache/
.parcel-cache/
.nyc_output/
